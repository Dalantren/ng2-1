            "tsConfig": "packages/ngx-qgrid/tsconfig.spec.json",
            "karmaConfig": "packages/ngx-qgrid/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/ngx-qgrid/tsconfig.lib.json",
              "packages/ngx-qgrid/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-qgrid-plugins": {
      "projectType": "library",
      "root": "packages/ngx-qgrid-plugins",
      "sourceRoot": "packages/ngx-qgrid-plugins/src",
      "prefix": "q-grid",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "packages/ngx-qgrid-plugins/tsconfig.lib.json",
