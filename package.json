{
  "name": "qgrid",
  "private": "true",
  "version": "13.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "build:app": "lerna run build --stream --scope ng2-qgrid-app",
    "start": "yarn clean && node ./scripts/serve.js",
    "lint": "lerna run lint --stream",
    "clean": "lerna run clean --stream",
    "test": "lerna run test --stream",
    "publish:alpha": "lerna publish --canary --preid alpha"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "codelyzer": "^6.0.2",
    "command-line-args": "^5.2.0",
    "fs": "^0.0.1-security",
    "karma": "^6.3.10",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "mocha": "^9.1.3",
    "path": "^0.12.7",
    "sane": "^5.0.1"
  }
}
