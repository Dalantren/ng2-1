{
  "extends": "../../tsconfig.lib.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ],
  "composite": true,
  "references": [
    {
      "path": "../qgrid-core"
    },
    {
      "path": "../qgrid-plugins"
    },
    {
      "path": "../ngx-qgrid"
    },
    {
      "path": "../ngx-qgrid-plugins"
    }
  ]
}