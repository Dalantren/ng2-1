{
	"extends": "../../.eslintrc.json",
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      "tab",
      {
        "SwitchCase": 1
      }
    ],
		"@typescript-eslint/adjacent-overload-signatures": "warn"
  }
}
